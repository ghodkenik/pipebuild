        {
            "type": "java",
            "name": "MergeIntervals",
            "request": "launch",
            "mainClass": "com.test.springtest.controller.MergeIntervals",
            "projectName": "pipebuild"
        },
        {
            "type": "java",
            "name": "MiscTest",
            "request": "launch",
            "mainClass": "com.test.MiscTest",
